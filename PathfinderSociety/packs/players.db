},"propertyRune2":{"value":""},"propertyRune3":{"value":""},"propertyRune4":{"value":""},"property1":{"value":"","dice":null,"die":"","damageType":"","critDice":0,"critDie":"","critDamage":"","critDamageType":"","criticalConditionType":"","criticalConditionValue":null,"strikeConditionType":"","strikeConditionValue":null}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"WLBr41D9DZtcz9yW":3},"flags":{"core":{"sourceId":"Compendium.pf2e.equipment-srd.FPwsiGqMCNPLHmjX"}}},{"_id":"J23edNR2fHmN7h1x","name":"Blowgun Darts","type":"consumable","img":"systems/pf2e/icons/equipment/weapons/blowgun-dart.webp","data":{"description":{"value":"<p>These thin, light darts are typically made of hardwood and stabilized with fletching of down or fur. They are often hollow so they can be used to deliver poison.</p>"},"source":{"value":"Pathfinder Core Rulebook"},"traits":{"value":[],"rarity":"common","custom":""},"rules":[],"slug":"blowgun-darts","schema":{"version":0.768,"lastMigration":{"datetime":"2022-07-28T21:33:03.625-06:00","version":{"schema":0.764,"foundry":"9.269","system":"3.13.2.13"}}},"quantity":30,"baseItem":null,"hp":{"brokenThreshold":0,"value":0,"max":0},"hardness":0,"weight":{"value":"-"},"equippedBulk":{"value":""},"price":{"value":{"cp":5},"per":10},"equipped":{"carryType":"worn","handsHeld":0},"stackGroup":"blowgunDarts","negateBulk":{"value":"0"},"containerId":null,"preciousMaterial":{"value":""},"preciousMaterialGrade":{"value":""},"size":"med","identification":{"status":"identified","unidentified":{"name":"","img":"","data":{"description":{"value":""}}},"misidentified":{}},"usage":{"value":"held-in-one-hand"},"activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":0,"per":null,"autoUse":true,"autoDestroy":true},"spell":{"data":null,"heightenedLevel":null},"level":{"value":0},"consumableType":{"value":"ammo